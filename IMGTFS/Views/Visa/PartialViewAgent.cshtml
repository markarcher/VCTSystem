                        <th>Currency</th>


                    </tr>
                </thead>
                @foreach (var item in ViewBag.list)
                {
                    <tr>
                        <td>@item.Account_No</td>
                        <td>@item.AgentCode </td>
                        <td>@item.LastName</td>
                        <td>@item.FirstName</td>
                        <td>@item.Addr1</td>
                        <td>@item.Addr2</td>
                        <td>@item.Addr3</td>
                        <td>@item.Addr4</td>
                        <td>@item.Phone1</td>
                        <td>@item.Phone2</td>
                        <td>@item.Sex</td>
                        <td>@item.Birthday</td>
                        <td>@item.PayrollCD</td>
                        <td>@item.LASTMEMB</td>
                        <td>@item.Status</td>
                        <td>@item.CBCODE</td>
                        <td>@item.PROFITCNTR</td>
                        <td>@item.ATM</td>
                        <td>@item.TAXEXEMPT</td>
                        <td>@item.CURR_BAL</td>
                        <td>@item.HOLDAMT</td>
                        <td>@item.JOINTSTAT</td>
                        <td>@item.Sector</td>
                        <td>@item.ACCT_TYPE</td>
                        <td>@item.Currency</td>

                    </tr>
                }
            </table>
    }
    else
    {
        <table id="myTableAgentSupport" class="cell-border" cellspacing="0">
            <thead>
            <tr>
                <th>#</th>
