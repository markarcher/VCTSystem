                        <th> Account No </th>
                        <th>Agent Code </th>
                        <th> Lastname</th>
                        <th>firstname</th>
                        <th>Address1 </th>
                        <th>Address2</th>
                        <th>Address3</th>
                        <th>Address4</th>
                        <th>Phone1</th>
                        <th>Phone2</th>
                        <th>Sex</th>
                        <th>Birthday</th>
                        <th>PayrollCD</th>
                        <th>LASTMEMB</th>
                        <th>Status</th>
                        <th>CBCODE</th>
                        <th>PROFITCNTR</th>
                        <th>ATM</th>
                        <th>TAXEXEMPT</th>
                        <th>CURR_BAL</th>
                        <th>HOLDAMT</th>
                        <th>JOINTSTAT</th>
                        <th>Sector</th>
